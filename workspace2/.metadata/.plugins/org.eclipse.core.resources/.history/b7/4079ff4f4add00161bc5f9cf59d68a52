package first_maven;

public class study {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
	     int res = 0;
	        int tem = 0;
	        for(int i = 0;i < 31;i++){
	            if(num == 0) break;
	            tem = ~(1&num);
	            System.out.println(tem);
	            res = res + (tem<<i);
	            num = num >> 1;
	            System.out.println(res);
	            
	        }
	}

}
