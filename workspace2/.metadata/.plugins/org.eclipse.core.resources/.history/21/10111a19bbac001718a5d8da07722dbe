package first_maven;

import java.math.BigInteger;

public class Main{
    public static int hammingWeight(int n) {
        int ones = 0;
        while(n!=0) {
            ones = ones + (n & 1);
            n = n>>>1;
        }
        return ones;
    }
    public static void main(String[] args) {
        BigInteger bi = new BigInteger("1234567890123123");
        System.out.println(bi);
    }
}
