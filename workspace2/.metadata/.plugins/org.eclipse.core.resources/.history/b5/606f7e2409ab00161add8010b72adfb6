package os;

public class test {

	public static void main(String[] args) {
		 int n = 27;
		 int a[] = new int[10];
	        String ret="";
	        int positon=0;
	        for(;n>0;n/=27)
	        {
	            a[positon++] = n%27;
	        }
	        positon--;
	        for(;positon>=0;positon--)
	        {
	        	System.out.println(a[positon]);
	            ret+=(char)(a[positon]+'A'-1);
	        }
	      
	System.out.println(ret);
	
	}

}
