package first_maven;

import java.util.*;
 class TreeNode {
	      int val;
	    TreeNode left;
	    TreeNode right;
	     TreeNode(int x) { val = x; }
 }

public class Demo {
	  static void swap(int[] a,int i, int j)
	 {
		 int temp = a[i];
		  a[i] = a[j];
		  a[j] = temp;
		 
	 }
	public static void main(String[] args) {
		int[] nums = {1, 0, 3, 2, -1, -4};
		int temp;
		for(int i = nums.length - 1; i >= 1; i--)
		{
			for(int j = 0; j <= i - 1; j++)
			{
				if(nums[j] > nums[j + 1])
				{
					temp = nums[j];
					nums[j] = nums[j + 1];
					nums[j + 1] = temp;
				}
			}
		}
		for(int i = 0; i < nums.length; i++)
			System.out.println(nums[i]);
	
	}

}
