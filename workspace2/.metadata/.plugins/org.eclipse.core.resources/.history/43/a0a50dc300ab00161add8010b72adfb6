package os;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="A";
		char[] ch = s.toCharArray();
	     int ret=0;
	     for(int i=0;i<ch.length;i++)
	     {
	         ch[i]-=64;
	     }
	     for(int i=0;i<ch.length;i++)
	     {
	         ret=ch
	     }
	System.out.println(ret);
	
	}

}
