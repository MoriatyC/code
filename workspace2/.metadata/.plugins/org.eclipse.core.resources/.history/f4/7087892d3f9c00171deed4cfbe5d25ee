package first_maven;

import java.util.ArrayList;
import java.util.List;

public class Main {  
    static void f(String s) {
        s = "b";
    }
    static void ff(List<Integer> list) {
        list.add(2);
    }
    public static void main(String[] args) {
        String s = "a";
        List<Integer> list = new ArrayList<>();
        list.add(1);
        ff(list);
        System.out.println(list.size());
    }
}