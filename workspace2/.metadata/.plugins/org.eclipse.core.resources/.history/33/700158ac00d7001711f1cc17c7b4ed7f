public class Main{
    public static class Demo {
        static int count;
        static int sum;
        public Demo() {
            count++;
            sum += count;
        }
    }
    
    public static void main(String[] args)   {
        int[] a = {1,3,0,7,0};
        Arrays.sort(a);
        System.out.println(LastRemaining_Solution(5,3));
        
    }
}
