package first_maven;
import java.util.Random;
import java.util.*;
import java.util.Map.Entry;
import java.util.stream.IntStream;
 class TreeNode {
	      int val;
	    TreeNode left;
	    TreeNode right;
	     TreeNode(int x) { val = x; }
 }
   class ListNode {
	      int val;
	     ListNode next;
	      ListNode(int x) { val = x; }
	 }
public class Demo {

	public static void main(String[] args) {
		int c = 1;
		ListNode l = new ListNode(c++);
		ListNode h = l;
		for(int i = 1; i < 5; i++)
		{
			l.next = new ListNode(c++);
			l = l.next;
		}
		l = h;
		
		for(int i = 1; i < 3; i++)
		{
			l = l.next;
			System.out.println(l.val);
			
		}
	      
		   l = l.next;
		   l = h;
			for(int i = 0; i < 5; i++)
			{
				l = l.next;
				System.out.println(l.val);
			}
		   
	}

}


 
