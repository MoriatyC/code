package first_maven;

import java.util.Arrays;

public class Main{
    public static class Demo {
        static int count;
        static int sum;
        public Demo() {
            System.out.println("heh");
            count++;
            sum += count;
        }
    }
    
    public static void main(String[] args)   {
        
        Demo[] arr = new Demo[5];
        Demo a = new Demo();
    }
}
