package os;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=0 ;
		 if (n == 0) System.out.println(0); 		    
		    int result = 0;
		    for (int i = 0; i < 32; i++) {
		        result <<= 1;
		        if ((n & 1) == 1) result++;
		        n >>= 1;
		    }
		   System.out.println(result);
	}

}
