package first_maven;

public class study {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=3;
		int b=1;
		int tem = a^b;
		int res = 0;
		 for(int i = 0;i<32;i++)
	        {
	           if(1==(tem&1)){
	               res++;
	           }
	           tem=tem>>2;
	        }
		System.out.println(res);

	}

}
