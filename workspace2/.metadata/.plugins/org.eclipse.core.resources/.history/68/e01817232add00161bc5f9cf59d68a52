package first_maven;

public class study {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=1;
		int b=4;
		int tem = a^b;
		int res = 0;
		 for(int i = 0;i>32;i++)
	        {
	           if((tem&1)==1){
	               res++;
	               System.out.println(res);
	           }
	           tem=tem/2;
	        }
//		System.out.println(tem&1);

	}

}
