package first_maven;

class ListNode {
     int val;
     ListNode next;
     ListNode(int x) {
         val = x;
         next = null;
     }
 }
public class Main {  
    public static void solution(char[] string, int length) {
        if (string == null || string.length == 0) {
            return;
        }
        int countBank = 0;
        for (int i = 0; i < length; i++) {
            if (string[i] == ' ') {
                countBank++;
            }
        }
        int newLength = length + 2 * countBank;
        length--;
        newLength--;
        while (length >=0 && length != newLength) {
            if (string[length] != ' ') {
                string[newLength--] = string[length--];
            } else {
                string[newLength--] = '0';
                string[newLength--] = '2';
                string[newLength--] = '%';
                length--;
            }
        }
        
    }
  static  char  c;
    public static void main(String[] args) {
        char[] string = new char[50];
        String s = null;
        String s2 = "";
        String s3 = "we are   happy";
        String s4 = "we are happy";
        int index = 0;
        char[] tmp = s2.toCharArray();
        for (char c: tmp) {
            string[index++] = c;
        }
        solution(string, s.length());
        System.out.println(String.valueOf(string));
    }
}