package first_maven;

import java.util.Arrays;


class Base{
    public Base(String s){
        System.out.print("B");
    }
}
public class Main extends Base{
    public Main (String s) {
        super(s);
        System.out.print("D");
    }
    public static void main(String[] args){
        char c = '1'-'0';
        System.out.println();
    }
}