package first_maven;
import java.util.Random;
import java.util.*;
import java.util.Map.Entry;
import java.util.stream.IntStream;
 class TreeNode {
	      int val;
	    TreeNode left;
	    TreeNode right;
	     TreeNode(int x) { val = x; }
 }
   class ListNode {
	      int val;
	     ListNode next;
	      ListNode(int x) { val = x; }
	 }
public class Demo {

	public static void main(String[] args) {
		int[] nums = {4,14,2};
		int ret = 0;
	     int total = 0;
	     int len = nums.length;
	        StringBuffer[] bn = new StringBuffer[nums.length + 1];
	        int temp;
	        for(int i = 0; i < nums.length; i++)
	        {
	            temp = nums[i];
	            total = total | nums[i];
	            bn[i] = new StringBuffer();
	            while(temp > 0)
	            {
	            	
	                bn[i].append(temp & 1);
	                temp = temp >> 1;
	            }
	            
	        }
	        temp = total;
	        bn[len] = new StringBuffer();
            while(temp > 0)
            {
                bn[len].append(temp & 1);
                temp = temp >> 1;
            }
            for(int i = 0; i <= nums.length; i++)
            {
                System.out.println(bn[i].toString());
            }
            for(int i = 0; i < len; i++)
            {
            	for(int j = bn[i].length() - 1; j >= 0; j--)
            	{
            		
            	}
            }
	      
		   
	}

}


 
