package os;

public class test {

	public static void main(String[] args) {
		 int n = 53;
		
		 int a[] = new int[10];
	        String ret="";
	        int positon=0;
	        for(;n>0;n/=26)
	        {
	            a[positon++] = n%26;
	        }
	        positon--;
	        for(;positon>=0;positon--)
	        {
	        	if(a[positon]==1&&a[positon-1]==0)
	        	{
	        		ret+='Z';
	        		positon--;
	        	}
	        	else
	            ret+=(char)(a[positon]+'A'-1);
	        }
	      
	System.out.println(ret);
	
	}

}
